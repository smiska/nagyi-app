{
  "name": "@smiska/nagyi-app",
  "version": "1.0.0",
  "description": "In which I try to include my learning almost every day, but in worst case weekly.",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "server": "nodemon index.js --ignore tests",
    "client": "npm run start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "build": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client",
    "heroku-postbuild": "npm run build",
    "test": "jest",
    "legacystart": "webpack-dev-server --open --config webpack.dev.js",
    "legacybuild": "webpack --config webpack.prod.js",
    "json:server": "json-server --watch db.json --port 3005"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/smiska/nagyi-app.git"
  },
  "author": "Mihaly Saroy <saroy.mihaly@gmail.com> (https://github.com/smiska)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/smiska/nagyi-app/issues"
  },
  "homepage": "https://github.com/smiska/nagyi-app#readme",
  "dependencies": {
    "aws-sdk": "^2.188.0",
    "axios": "^0.19.0",
    "body-parser": "^1.17.2",
    "concurrently": "^3.5.0",
    "cookie-session": "^2.0.0-beta.2",
    "express": "^4.15.3",
    "express-graphql": "^0.9.0",
    "graphql": "^14.5.8",
    "migrate-mongoose": "^3.2.2",
    "mongoose": "^4.11.1",
    "nodemon": "^1.11.0",
    "passport": "^0.3.2",
    "passport-google-oauth20": "^1.0.0",
    "path-parser": "^2.0.2",
    "react": "^16.8.6",
    "redis": "^2.8.0",
    "uuid": "^3.2.1"
  },
  "devDependencies": {
    "bootstrap": "^4.3.1",
    "clean-webpack-plugin": "^2.0.2",
    "create-react-app": "^3.1.2",
    "css-loader": "^2.1.1",
    "file-loader": "^3.0.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.9.0",
    "jquery": "^3.4.1",
    "json-server": "^0.15.1",
    "mini-css-extract-plugin": "^0.6.0",
    "node-sass": "^4.12.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "popper.js": "^1.15.0",
    "puppeteer": "^1.0.0",
    "sass-loader": "^7.1.0",
    "storybook": "^5.1.11",
    "style-loader": "^0.23.1",
    "webpack": "^4.31.0",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.3.1",
    "webpack-merge": "^4.2.1"
  }
}
